{
    "name": "glofox/cakephp-amazon-s3",
    "description": "Legacy and Deprecated Amazon S3 Plugin for CakePHP",
    "type": "cakephp-plugin",
    "keywords": ["cakephp", "aws", "s3"],
    "homepage": "https://bitbucket.com/glofox/cakephp-amazon-s3",
    "license": "MIT",
    "authors": [
        {
            "name": "Giancarlo Sanchez",
            "homepage": "http://twitter.com/gianksp",
            "role": "Author"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "aws/aws-sdk-php": "^3.23",
        "aws/aws-php-sns-message-validator": "^1.1",
        "doctrine/cache": "^1.6"
    },
    "require-dev": {
		"phpunit/phpunit": "3.7.*"
	},
    "extra": {
        "installer-name": "AmazonS3"
    }
}
